{
  "name": "@chimeric/source",
  "version": "0.0.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dataquail/chimeric.git"
  },
  "packageManager": "pnpm@10.7.0",
  "type": "module",
  "scripts": {
    "build:all": "nx run-many --target=build",
    "build:packages": "nx run-many --target=build --exclude=examples/**",
    "build:examples": "nx run-many --target=build --projects=examples/todo-app",
    "test:all": "nx run-many --target=test",
    "test:packages": "nx run-many --target=test --exclude=examples/**",
    "test:examples": "nx run-many --target=test --projects=examples/todo-app",
    "lint:all": "nx run-many --target=lint",
    "lint:packages": "nx run-many --target=lint --exclude=examples/**",
    "lint:examples": "nx run-many --target=lint --projects=examples/todo-app",
    "watch:all": "pnpm run build:all && nx watch --all -- pnpm run build:all",
    "watch:packages": "pnpm run build:packages && nx watch --exclude=examples/** -- pnpm run build:packages",
    "watch:examples": "pnpm run build:examples && nx watch --projects=examples/todo-app -- pnpm run build:examples",
    "dev:all": "pnpm run watch:all",
    "dev:examples": "pnpm run watch:examples",
    "typecheck:all": "nx run-many --target=typecheck --exclude=examples/**",
    "precommit": "pnpm run lint:all && pnpm run test:all && pnpm run typecheck:all",
    "prettier": "prettier --ignore-unknown '**/*'",
    "prettier:write": "pnpm run prettier --write",
    "serve:todo-app": "pnpm run build:examples && npx nx serve examples/todo-app --base=/todo-app",
    "troubleshoot:ci": "act -W .github/workflows/ci.yml workflow_dispatch",
    "prepare": "husky"
  },
  "private": true,
  "devDependencies": {
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@eslint-react/eslint-plugin": "^1.19.0",
    "@eslint/js": "^9.23.0",
    "@nx/eslint": "21.1.2",
    "@nx/eslint-plugin": "21.1.2",
    "@nx/js": "21.1.2",
    "@nx/react": "21.1.2",
    "@nx/vite": "21.1.2",
    "@nx/web": "21.1.2",
    "@swc-node/register": "~1.9.2",
    "@swc/cli": "0.6.0",
    "@swc/core": "~1.5.29",
    "@swc/helpers": "~0.5.15",
    "@tanstack/react-query": "^5.69.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^14.3.1",
    "@types/node": "^20.17.27",
    "@types/react": "^19.0.12",
    "@types/react-dom": "19.0.0",
    "@types/uuid": "^9.0.8",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^3.0.9",
    "@vitest/ui": "^3.0.9",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "10.1.5",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jsx-a11y": "6.10.1",
    "eslint-plugin-react": "7.35.0",
    "eslint-plugin-react-hooks": "5.0.0",
    "husky": "^9.1.7",
    "jiti": "2.4.2",
    "jsdom": "~22.1.0",
    "jsonc-eslint-parser": "^2.4.0",
    "nx": "21.1.2",
    "prettier": "^2.8.8",
    "tslib": "^2.8.1",
    "typescript": "~5.7.3",
    "typescript-eslint": "^8.28.0",
    "verdaccio": "^6.1.0",
    "vite": "^6.2.3",
    "vite-plugin-dts": "~4.5.3",
    "vitest": "^3.0.9"
  },
  "nx": {
    "includedScripts": [],
    "targets": {
      "local-registry": {
        "executor": "@nx/js:verdaccio",
        "options": {
          "port": 4873,
          "config": ".verdaccio/config.yml",
          "storage": "tmp/local-registry/storage"
        }
      }
    }
  },
  "dependencies": {}
}
