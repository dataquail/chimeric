{
  "name": "@chimeric/testing-core",
  "version": "0.2.13",
  "description": "Core testing utilities for chimeric interfaces",
  "author": "zacharyweidenbach",
  "license": "MIT",
  "keywords": [
    "testing",
    "react",
    "typescript",
    "test-harness",
    "react-testing-library",
    "interface",
    "ioc",
    "di",
    "dependency injection"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/dataquail/chimeric.git",
    "directory": "packages/testing-core"
  },
  "homepage": "https://dataquail.github.io/chimeric",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "typings": "dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js",
      "default": "./dist/index.esm.js"
    }
  },
  "dependencies": {
    "@chimeric/core": "workspace:*"
  },
  "peerDependencies": {
    "@testing-library/react": "^14.3.1",
    "react": "^18 || ^19"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": false
    }
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.4.5",
    "@testing-library/react": "^14.3.1",
    "react": "^18 || ^19"
  }
}
